{
  "name": "woocommerce-product-sync",
  "version": "1.0.0",
  "description": "Node.js script to sync WooCommerce products by SKU using REST API",
  "type": "module",
  "main": "dist/woocommerce-sync.js",
  "types": "dist/woocommerce-sync.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/woocommerce-sync.js",
    "dev": "tsx woocommerce-sync.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "woocommerce",
    "sync",
    "products",
    "sku",
    "rest-api",
    "wordpress",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=22.18.0"
  }
}
